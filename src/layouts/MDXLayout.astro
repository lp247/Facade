---
import FrameWithHeading from "@layouts/FrameWithHeading.astro";
import type {MDXLayoutProps} from "astro";

interface MDXFrontmatter {
    heading: string;
    subheading?: string;
}

const {frontmatter} = Astro.props as MDXLayoutProps<MDXFrontmatter>;
---

<FrameWithHeading heading={frontmatter.heading} subheading={frontmatter.subheading}>
    <slot />
</FrameWithHeading>
