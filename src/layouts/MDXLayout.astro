---
import WithHeading from "@layouts/WithHeading.astro";
import type {MDXLayoutProps} from "astro";

interface MDXFrontmatter {
    heading: string;
    subheading?: string;
}

const {frontmatter} = Astro.props as MDXLayoutProps<MDXFrontmatter>;
---

<WithHeading heading={frontmatter.heading} subheading={frontmatter.subheading}>
    <slot />
</WithHeading>
